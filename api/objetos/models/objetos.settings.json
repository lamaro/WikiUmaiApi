{
  "kind": "collectionType",
  "collectionName": "objetos",
  "info": {
    "name": "Objetos"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "nombre": {
      "type": "string"
    },
    "planeta": {
      "model": "planetas",
      "via": "objetos"
    },
    "slug": {
      "type": "string"
    },
    "personajes": {
      "collection": "personajes",
      "via": "objetos"
    },
    "tipo": {
      "type": "enumeration",
      "enum": [
        "profesion",
        "inventario"
      ],
      "unique": false
    },
    "hitos": {
      "via": "objetos",
      "collection": "hitos",
      "dominant": true
    },
    "origen": {
      "type": "richtext"
    },
    "contexto_uso": {
      "type": "richtext"
    },
    "modo_uso": {
      "type": "richtext"
    },
    "impacto_social": {
      "type": "richtext"
    }
  }
}
